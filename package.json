{
  "name": "dice-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "Electron.cjs",
  "homepage": "./",
  "scripts": {
    "dev": "vite --port 3000 --mode development",
    "test": "vite --port 3000 --mode staging",
    "build": "tsc && vite build --mode development",
    "build:dev": "tsc && vite build --mode development",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "preview": "vite preview",
    "docker": "docker build --no-cache --rm -f Dockerfile -t pinomaker/dice-frontend:latest . --platform linux/amd64",
    "docker-push": "docker push pinomaker/dice-frontend:latest",
    "docker-buildx": "docker buildx build --platform linux/arm64,linux/amd64 -t pinomaker/dice-frontend:latest-arm64 -t pinomaker/dice-frontend:latest-amd64 --file Dockerfile . --push",
    "jest": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.14.18",
    "@mui/styles": "^5.13.2",
    "@mui/system": "^5.13.2",
    "@mui/x-tree-view": "^6.17.0",
    "@reduxjs/toolkit": "^1.9.5",
    "async-mutex": "^0.4.0",
    "axios": "^1.6.1",
    "firebase": "^10.3.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-editor-ajrm": "^2.5.14",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.12.1",
    "reactflow": "^11.10.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.2.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
